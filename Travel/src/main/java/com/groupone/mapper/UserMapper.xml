<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.groupone.mapper.UserMapper">

	<select id="selectUser" resultType="User">
		SELECT * 
		FROM tbl_user
		WHERE user_id = #{id} 
	</select>
	
</mapper>

